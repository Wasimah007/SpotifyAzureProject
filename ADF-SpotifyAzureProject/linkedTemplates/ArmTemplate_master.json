{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-SpotifyAzureProject"
		},
		"AzureSqlDatabaseLKSRC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseLKSRC'"
		},
		"datalakeSinkLK_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakeSinkLK'"
		},
		"AzureSqlDatabaseLKSRC_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "azuresqlprojectserversrc.database.windows.net"
		},
		"AzureSqlDatabaseLKSRC_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "azureprojectspotify"
		},
		"AzureSqlDatabaseLKSRC_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminwaseem"
		},
		"datalakeSinkLK_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuresqlproject.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-SpotifyAzureProject_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLKSRC_password": {
						"value": "[parameters('AzureSqlDatabaseLKSRC_password')]"
					},
					"datalakeSinkLK_accountKey": {
						"value": "[parameters('datalakeSinkLK_accountKey')]"
					},
					"AzureSqlDatabaseLKSRC_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseLKSRC_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseLKSRC_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseLKSRC_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseLKSRC_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseLKSRC_properties_typeProperties_userName')]"
					},
					"datalakeSinkLK_properties_typeProperties_url": {
						"value": "[parameters('datalakeSinkLK_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}